{
    "routes": [
        {
            "route": "/",
            "redirect": "/login",
            "statusCode": 301
        }, 

        {
            "route": "/home",
            "allowedRoles": ["authenticated"]
        }, 
        {
            "route": "/.auth/login/done",
            "redirect": "/home",
            "allowedRoles": ["authenticated"],
            "statusCode": 301
        }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
    }
  }

 
